{
  "name": "@adexchange/aeg-limelight-api",
  "version": "0.0.20",
  "description": "Limelight API",
  "main": "./lib/api.js",
  "author": "Justin Durham",
  "repository": {
    "type": "git",
    "url": "git://git@bitbucket.org:AdExchangeGrp/aeg-limelight-api.git"
  },
  "license": "UNLICENSED",
  "scripts": {
    "test": "mocha --recursive test/",
    "test:watch": "npm run test -- -w",
    "test:coverage": "istanbul cover _mocha -- --recursive test/",
    "jshint": "jshint .",
    "jshint:watch": "npm run jshint && onchange \"**/*.js\" \".jshint*\" -- npm run jshint",
    "build": "npm run jshint && istanbul cover _mocha -- --recursive --reporter mocha-junit-reporter test/unit",
    "patch": "git reset --hard HEAD && git clean -fd && npm version patch -m \"[ci skip] %s\" && git push origin master && git push origin master --tags && npm publish"
  },
  "devDependencies": {
    "istanbul": "^0.3.17",
    "jshint": "^2.9.2",
    "mocha": "^2.2.5",
    "mocha-junit-reporter": "^1.9.1",
    "onchange": "^1.1.0",
    "parallelshell": "^1.1.1",
    "should": "^5.2.0"
  },
  "dependencies": {
    "@adexchange/aeg-logger": "0.0.24",
    "config": "^1.12.0",
    "querystring": "^0.2.0",
    "request": "^2.53.0",
    "stringformat": "0.0.5",
    "underscore": "^1.8.2"
  },
  "engine": "node >= 4.1.0"
}
