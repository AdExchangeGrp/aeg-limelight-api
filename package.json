{
  "name": "@adexchange/aeg-limelight-api",
  "version": "0.0.19",
  "description": "Limelight API",
  "main": "./lib/api.js",
  "author": "Justin Durham",
  "repository": {
    "type": "git",
    "url": "git://git@bitbucket.org:AdExchangeGrp/aeg-limelight-api.git"
  },
  "license": "UNLICENSED",
  "scripts": {
    "test": "mocha --recursive test/",
    "test:watch": "npm run test -- -w",
    "test:coverage": "istanbul cover _mocha -- --recursive test/",
    "jshint": "jshint .",
    "jshint:watch": "npm run jshint && onchange \"**/*.js\" \".jshint*\" -- npm run jshint",
    "dev": "parallelshell \"npm run jshint:watch\" \"npm run test:watch\"",
    "build": "npm run jshint && mocha --recursive  --reporter mocha-junit-reporter test/unit/",
    "postversion": "git push && git push --tags && npm publish ."
  },
  "devDependencies": {
    "should": "^5.2.0",
    "mocha": "^2.2.5",
    "mocha-junit-reporter": "^1.9.1",
    "parallelshell": "^1.1.1",
    "onchange": "^1.1.0",
    "istanbul": "^0.3.17"
  },
  "dependencies": {
    "@adexchange/aeg-logger": "0.0.24",
    "config": "^1.12.0",
    "querystring": "^0.2.0",
    "request": "^2.53.0",
    "stringformat": "0.0.5",
    "underscore": "^1.8.2"
  },
  "engine": "node >= 4.1.0"
}
