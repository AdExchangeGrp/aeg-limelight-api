{
  "name": "@adexchange/aeg-limelight-api",
  "version": "0.0.1",
  "description": "Limelight API",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "author": "Ad Exchange Group",
  "repository": {
    "type": "git",
    "url": "https://github.com/AdExchangeGrp/aeg-limelight-api.git"
  },
  "license": "UNLICENSED",
  "scripts": {
    "test": "mocha --recursive --compilers ts:ts-node/register test/**/*.ts",
    "test:coverage": "nyc npm run test",
    "lint": "tslint src/**/*.ts",
    "lint:watch": "npm run lint && onchange \"**/*.ts\" \"tslint.json\" -- npm run lint",
    "compile": "tsc",
    "compile:watch": "npm run compile -- --watch",
    "dev": "npm run compile:watch",
    "build:test": "mocha --recursive --compilers ts:ts-node/register --reporter mocha-junit-reporter test/**/*.ts",
    "build:coverage": "nyc npm run build:test"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "html"
    ],
    "all": true
  },
  "devDependencies": {
    "@types/mocha": "^2.2.41",
    "@types/node": "^6.0.0",
    "mocha": "^3.2.0",
    "mocha-junit-reporter": "^1.13.0",
    "nyc": "^11.0.3",
    "onchange": "^3.2.1",
    "should": "^11.2.0",
    "ts-node": "^3.2.0",
    "tslint": "^5.5.0",
    "typescript": "^2.4.1"
  },
  "dependencies": {
    "@adexchange/aeg-common": "0.0.282",
    "@types/config": "0.0.32",
    "@types/lodash": "^4.14.76",
    "@types/request": "^2.0.3",
    "@types/request-promise": "^4.1.38",
    "config": "^1.26.1",
    "fast-csv": "^2.4.0",
    "lodash": "^4.17.4",
    "moment": "^2.19.2",
    "querystring": "^0.2.0",
    "request": "^2.81.0",
    "request-promise": "^4.2.0",
    "stringformat": "0.0.5"
  },
  "engine": "node >= 6.10.0"
}
